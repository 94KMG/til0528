fun countAlphabet(alphabet: String) : Int {
    var alphabetSet = mutableSetOf<String>()
    var newAlphabetSet = mutableSetOf<String>()
    var sameAlphabet = ""
    for (i in alphabet){
        alphabetSet.add(i.toString().uppercase())
    }

    for (i in alphabetSet){
        newAlphabetSet.add(i)
    }
    return newAlphabetSet.size
}

countAlphabet("appleAPPLE")
